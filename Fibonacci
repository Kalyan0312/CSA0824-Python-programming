def Fibonacci(n):
    if n<0:
        print("Incorrect input")
    elif n==0:
        return 0
    elif n==1 or n==2:
        return 1
    else:
        return Fibonacci(n-1)+Fibonacci(n-2)
#Driver program 
num=int(input("Enter the number of terms="))
for i in range(num):
    print(Fibonacci(i))
Enter the number of terms=8
